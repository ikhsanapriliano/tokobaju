{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "data source=mysource;user=myuser;password=mypassword;database=mydatabase;TrustServerCertificate=True"
  },
  "JwtSettings": {
    "Key": "mysecret",
    "Issuer": "myissuer",
    "ExpiresinMinutes": 60
  }
}
